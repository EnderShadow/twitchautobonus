{
   "name":"Twitch Auto Bonus",
   "description":"Automatically redeems bonus Channel Points.",
   "version":"1.0",
   "manifest_version":2,
   "permissions":[
      "https://www.twitch.tv/*"
   ],
   "content_scripts":[
      {
         "matches":[
            "https://www.twitch.tv/*"
         ],
         "js":[
            "injected.js"
         ],
         "all_frames":true
      }
   ],
   "icons":{
      "128":"128.png"
   }
}